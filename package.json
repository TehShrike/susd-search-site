{
  "name": "susd-search-site",
  "private": true,
  "version": "1.0.0",
  "description": "Quickly search the Shut Up And Site Down archives",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify client/index.js -t stringify -o public/build.js",
    "watch": "watchify client/index.js -t stringify -o public/build.js -d",
    "startdev": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TehShrike/susd-search-site.git"
  },
  "keywords": [
    "su&sd",
    "shut",
    "up",
    "and",
    "sit",
    "down",
    "search",
    "tabletop"
  ],
  "author": "TehShrike",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/TehShrike/susd-search-site/issues"
  },
  "homepage": "https://github.com/TehShrike/susd-search-site#readme",
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "6.16.0",
    "babel-polyfill": "6.16.0",
    "babel-register": "6.16.3",
    "denodeify": "1.2.1",
    "ecstatic": "2.1.0",
    "koa": "2.0.0-alpha.7",
    "koa-compress": "2.0.0",
    "koa-router": "7.0.1",
    "koa-static": "3.0.0",
    "susd-page-parser": "1.0.0"
  },
  "devDependencies": {
    "abstract-state-router": "5.14.0",
    "babel-preset-es2015": "6.14.0",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "nodemon": "1.11.0",
    "ractive": "0.8.1",
    "ractive-state-router": "6.1.2",
    "stringify": "5.1.0",
    "watchify": "3.7.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": "es2015",
          "plugins": [
            "transform-async-to-generator"
          ]
        }
      ]
    ]
  }
}
