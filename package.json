{
  "name": "susd-search-site",
  "engines": {
    "node": ">=7.7.2"
  },
  "private": true,
  "version": "1.0.0",
  "description": "Quickly search the Shut Up And Sit Down archives",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify client/index.js -o public/build.js",
    "watch": "watchify client/index.js -o public/build.js -d",
    "start": "node server.js",
    "startdev": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TehShrike/susd-search-site.git"
  },
  "keywords": [
    "su&sd",
    "shut",
    "up",
    "and",
    "sit",
    "down",
    "search",
    "tabletop"
  ],
  "author": "TehShrike",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/TehShrike/susd-search-site/issues"
  },
  "homepage": "https://github.com/TehShrike/susd-search-site#readme",
  "dependencies": {
    "abstract-state-router": "~5.14.1",
    "babel-plugin-transform-async-to-generator": "6.16.0",
    "babel-preset-es2015": "6.24.1",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "download": "5.0.2",
    "key-master": "2.0.1",
    "koa": "2.5.2",
    "koa-compress": "2.0.0",
    "koa-conditional-get": "2.0.0",
    "koa-router": "7.0.1",
    "koa-static": "5.0.0",
    "level-mem": "3.0.1",
    "levelup-cache": "2.3.0",
    "loud-rejection": "1.6.0",
    "mkdirp": "0.5.1",
    "ms": "0.7.2",
    "os-tmpdir": "1.0.2",
    "p-filter": "1.0.0",
    "p-map": "1.0.0",
    "ractive": "0.8.3",
    "ractive-lazyload-img": "1.0.0",
    "ractive-select-on-focus": "1.1.0",
    "ractive-state-router": "~7.1.0",
    "sanitize-filename": "1.6.1",
    "stringify": "5.2.0",
    "susd-page-parser": "1.0.0",
    "then-denodeify": "1.0.0",
    "then-nodeify": "1.0.1",
    "url-join": "1.1.0"
  },
  "devDependencies": {
    "nodemon": "1.18.3",
    "watchify": "3.11.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": "es2015",
          "plugins": [
            "transform-async-to-generator"
          ]
        }
      ],
      [
        "stringify"
      ]
    ]
  }
}
